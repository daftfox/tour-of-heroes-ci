{
  "name": "tour-of-heroes",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run checkenv && tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "checkenv": "echo \"NODE_ENV=\"$NODE_ENV $PORT",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean": "rm -rf dist",
    "typings": "typings install",
    "serve": "http-server . -p 5557",
    "postinstall": "npm run typings",
    "build": "tsc -p tsconfig.json",
    "rollup": "rollup -f iife -c -o dist/bundle.es2015.js",
    "es5": "tsc --target es5 --allowJs dist/bundle.es2015.js --out dist/bundle.js",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "build_prod": "npm run clean && npm run build && npm run rollup && npm run es5 && npm run minify",
    "test": "karma start karma.conf.js"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.5",
    "systemjs": "0.19.27",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.13",
    "angular2-in-memory-web-api": "0.0.15",
    "bootstrap": "^3.3.6",
    "typings": "^1.0.4",
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "copy": "^0.3.0",
    "es6-shim": "^0.35.1",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.5.0",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-nyan-reporter": "^0.2.4",
    "karma-phantomjs-launcher": "^1.0.2",
    "remap-istanbul": "^0.6.4",
    "rollup": "^0.34.13",
    "rollup-plugin-node-resolve": "^2.0.0",
    "systemjs": "^0.19.27",
    "typescript": "^1.8.10",
    "uglifyjs": "^2.4.10"
  }
}
